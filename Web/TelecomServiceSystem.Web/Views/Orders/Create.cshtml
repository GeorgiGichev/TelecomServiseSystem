@model TelecomServiceSystem.Web.ViewModels.Orders.OrderInputViewModel
@{ this.ViewData["Title"] = "Choose Service Type"; }

@if (Model.Services.ServiseType == 1)
{
    <partial name="_Mobile" model="@Model"/>
}
else if(Model.Services.ServiseType == 2)
{
    <partial name="_Fixed" model="@Model"/>
}


